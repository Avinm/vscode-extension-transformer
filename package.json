{
    "name": "transformer",
    "displayName": "Transformer",
    "description": "Filtering, Selection and Text Transformations",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/dakaraphi/vscode-extension-transformer"
    },
    "publisher": "dakara",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:dakara-transformer.sortLines",
        "onCommand:dakara-transformer.filter",
        "onCommand:dakara-transformer.align",
        "onCommand:dakara-transformer.commands",
        "onCommand:dakara-transformer.reverseLines",
        "onCommand:dakara-transformer.distinctLines"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "dakara-transformer.sortLines",
                "title": "Sort Lines",
                "category": "Transform"
            },
            {
                "command": "dakara-transformer.filter",
                "title": "Filter Lines As New Document",
                "category": "Transform"
            },
            {
                "command": "dakara-transformer.reverseLines",
                "title": "Reverse Lines",
                "category": "Transform"
            },
            {
                "command": "dakara-transformer.align",
                "title": "Align",
                "category": "Transform"
            },
            {
                "command": "dakara-transformer.commands",
                "title": "List all commands",
                "category": "Transform"
            },
            {
                "command": "dakara-transformer.distinctLines",
                "title": "Distinct Lines",
                "category": "Transform"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "compile_common": "tsc -watch -p ../vscode-extension-common",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "mocha": "^2.3.3",
        "typescript": "^2.7.0-dev.20171118",
        "vscode": "^1.1.8"
    },
    "dependencies": {
        "vscode-extension-common": "file:../vscode-extension-common"
    }
}
