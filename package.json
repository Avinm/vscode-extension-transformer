{
    "name": "transformer",
    "displayName": "Transformer",
    "description": "Filtering, Selection and Text Transformations",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/dakaraphi/vscode-extension-transformer"
    },     
    "publisher": "dakara",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:dakara-transformer.sortLines",
        "onCommand:dakara-transformer.sortLinesByColumn",
        "onCommand:dakara-transformer.filter"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {"command": "dakara-transformer.sortLines",            "title": "Transform: sort lines"}, 
            {"command": "dakara-transformer.sortLinesByColumn",    "title": "Transform: sort lines by column"},
            {"command": "dakara-transformer.filter",               "title": "Transform: filter lines to new document"}
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.3",
        "vscode": "^1.5.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "lodash.orderby": "^4.6.0"
    }
}